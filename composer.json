{
  "name": "mrthito/number-to-hindi",
  "description": "Laravel package to convert English numbers to Hindi number or Hindi text, Hindi month name and Hindi Money Format",
  "keywords": [
    "laravel",
    "Laravel-number-hindi",
    "number-hindi",
    "number-month"
  ],
  "homepage": "https://github.com/rakibdevs/number-to-hindi",
  "license": "MIT",
  "authors": [
    {
      "name": "Prashant Rijal",
      "email": "prashantrijal.com.np@gmail.com",
      "homepage": "https://prashantrijal.com.np"
    }
  ],
  "require": {
    "php": "^7.1|^7.2|^7.3|^7.4|^8.0|^8.1|^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MrThito\\Hindinumber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MrThito\\Hindinumber\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["MrThito\\Hindinumber\\NumberToHindiServiceProvider"]
    }
  },
  "minimum-stability": "stable"
}
